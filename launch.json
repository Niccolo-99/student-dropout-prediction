{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "prompttuning UniTS pretrain x128",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/run.py",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "env": {
        "CUDA_VISIBLE_DEVICES": "0",
        "TORCH_FORCE_NO_WEIGHTS_ONLY_LOAD": "1"
      },
      "args": [
        "--is_training",
        "1",
        "--model_id",
        "UniTS_ptune_x64_all_datasets",
        "--model",
        "UniTS",
        "--d_model",
        "64",
        "--prompt_num",
        "10",
        "--patch_len",
        "16",
        "--stride",
        "16",
        "--e_layers",
        "3",
        "--batch_size",
        "32",
        "--lradj",
        "prompt_tuning",
        "--prompt_tune_epoch",
        "2",
        "--train_epochs",
        "0",
        "--learning_rate",
        "3e-3",
        "--clip_grad",
        "100",
        "--pretrained_weight",
        "checkpoints/units_x64_supervised_checkpoint.pth",
        "--task_data_config_path",
        "data_provider/multi_task.yaml",
        "--debug",
        "disabled"
      ],
      "justMyCode": false
    },
    {
      "name": "Test UniTS supervised x64",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/run.py",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "env": {
        "CUDA_VISIBLE_DEVICES": "0",
        "TORCH_FORCE_NO_WEIGHTS_ONLY_LOAD": "1"
      },
      "args": [
        "--is_training",
        "0",
        "--debug",
        "disabled",
        "--model_id",
        "Test_checkpoint_UniTS_supervised_x128_pretrain_all_datasets",
        "--model",
        "UniTS",
        "--pretrain",
        "checkpoints\\units_x128_pretrain_checkpoint.pth",
        "--prompt_num",
        "10",
        "--patch_len",
        "16",
        "--stride",
        "16",
        "--e_layers",
        "3",
        "--d_model",
        "64",
        "--batch_size",
        "32",
        "--task_data_config_path",
        "data_provider/multi_task.yaml"
      ],
      "justMyCode": false
    },
    {
      "name": "Fine-Tune UniTS (custom dataset)",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/run.py",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "env": {
        "CUDA_VISIBLE_DEVICES": "0"
      },
      "args": [
        "--is_training",
        "1",
        "--model_id",
        "UniTS_finetune_only_dropout_datasets_x128 pretrained 5epochs",
        "--model",
        "UniTS",
        "--prompt_num",
        "10",
        "--patch_len",
        "16",
        "--stride",
        "16",
        "--e_layers",
        "3",
        "--d_model",
        "128",
        "--learning_rate",
        "5e-5",
        "--weight_decay",
        "5e-6",
        "--train_epochs",
        "5",
        "--batch_size",
        "32",
        "--acc_it",
        "1",
        "--lradj",
        "supervised",
        "--prompt_tune_epoch",
        "0",
        "--clip_grad",
        "100",
        "--debug",
        "disabled",
        "--project_name",
        "finetune_experiment_only_dropout_datasets_128x pretrained",
        "--task_data_config_path",
        "data_provider/only_dropout_datasets_task.yaml",
        "--pretrained_weight",
        "checkpoints/units_x128_pretrain_checkpoint.pth"
      ],
      "justMyCode": false
    },
    {
      "name": "Training UnitelmaDropout dataset",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/UNITS-main/run.py",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "env": {
        "CUDA_VISIBLE_DEVICES": "0",
        "CUDA_LAUNCH_BLOCKING": "1"
      },
      "args": [
        "--is_training",
        "1",
        "--model_id",
        "UniTS_supervised_x64_with_UnitelmaDropout",
        "--model",
        "UniTS",
        "--lradj",
        "supervised",
        "--prompt_num",
        "10",
        "--patch_len",
        "16",
        "--stride",
        "16",
        "--e_layers",
        "3",
        "--d_model",
        "64",
        "--learning_rate",
        "1e-4",
        "--weight_decay",
        "5e-6",
        "--train_epochs",
        "5",
        "--batch_size",
        "32",
        "--acc_it",
        "1",
        "--debug",
        "disabled",
        "--project_name",
        "supervised_learning",
        "--clip_grad",
        "100",
        "--task_data_config_path",
        "UniTS-main\\data_provider\\dropout_dataset_task.yaml"
      ],
      "justMyCode": false
    }
  ]
